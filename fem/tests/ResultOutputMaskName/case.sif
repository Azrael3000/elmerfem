! Test masked VTU output
!
! P.R. 20.8.2020

Header
  CHECK KEYWORDS Warn
  Mesh DB "." "cube"
  Include Path ""
  Results Directory ""
End

Simulation
  Max Output Level = 5

  Coordinate System = "Cartesian"
  Coordinate Mapping(3) = 1 2 3

  Simulation Type = "Steady State"
  Steady State Max Iterations = 1
  Output Intervals = 0
End

Constants
  Gravity(4) = 0 -1 0 9.82
  Stefan Boltzmann = 5.67e-08
End

Body 1
  Name = "Body1"
  Equation = 1
  Material = 1
  Body Force = 1 
End


Body Force 1
  Name = "Heating"
  Heat Source = 1
End


Equation 1
  Name = "Equations"
  Active Solvers(1) = 1 
End

Solver 1
  Equation = "Heat Equation"
  Variable = "Temperature"
  Linear System Solver = "Iterative"
  Linear System Iterative Method = "BiCGStab"
  Linear System Max Iterations = 1000
  Linear System Convergence Tolerance = 1.0e-08
  Linear System Abort Not Converged = True
  Linear System Preconditioning = "ILU0"
  Linear System Residual Output = 10
  Steady State Convergence Tolerance = 1.0e-05
  Nonlinear System Max Iterations = 1
End


Solver 2
  Equation = "ResultOutput"
  Exec Solver = After Saving
   
  Procedure = "ResultOutputSolve" "ResultOutputSolver"
  Output File Name = "case" 

!  Scalar Field 1 = String "Temperature"
!  Scalar Field 2 = String "XYZ"

  Binary Output = True

  Vtu format = Logical True
!  Gmsh format = Logical True
  Mask Name = String "Save This"
End

Material 1
  Name = "Material1"
  Density = 1
  Heat Conductivity = 1
End

Boundary Condition 1
  Name = "Sides"
  Target Boundaries(2) = 1 2 
  Temperature = 0
End

Boundary Condition 2
  Name = "Sides"
  Target Boundaries(2) = 3 4  
  Temperature = 0

! Save selected boundaries only
  Save This = Logical True
End

Boundary Condition 3
  Name = "Bot"
  Target Boundaries(1) = 5
  Save This = Logical True
End

Boundary Condition 5
  Name = "Top"
  Target Boundaries(1) = 6
  Save This = Logical False
End




Solver 1 :: Reference Norm = 3.68642527E-02
Solver 2 :: Reference Sums(6) = Real 29 3 1324 842 378300 486.13513134593023
Solver 2 :: Reference Norm = 1.0
